<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projet_mobile">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/spacex_launch_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/spacex_launch_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".presentation.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.SecondActivity"
            android:label="Launch List"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".presentation.DetailActivity"
            android:label="Launch Details"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>